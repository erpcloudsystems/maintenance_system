 relative;\n        }\n\n        @media print {\n            .main {\n                border: none !important;\n            }\n        }      \n    </style>\n\n    <!-- ====================== Barcode ============================== -->\n    <div class=\"main\">\n                \n                <div class=\"barcode_header\">\n                    \n                    <p class=\"text-center\">{{ doc.name }}</p>\n                </div>\n\n                <div class=\"text-center item_barcode\">\n                    <svg class=\"barcode\" jsbarcode-format=\"CODE128\" jsbarcode-value={{% set frappe.db.get_value(\"Maintenance Item\", {\"name\": doc.maintenance_item}, \"barcode\") %}} jsbarcode-height=\"40\"\n                        jsbarcode-width=\"2\" jsbarcode-fontsize=\"10\" jsbarcode-textmargin=\"10\" jsbarcode-margin=\"5\" jsbarcode-fontoptions=\"bold\">\n                    </svg>\n                </div>\n                \n        </div>\n    \n<script src=\"https://cdn.jsdelivr.net/npm/jsbarcode@3.11.0/dist/JsBarcode.all.min.js\"></script>\n<script>\n\nJsBarcode(\".barcode\").init();\n</script>\n<!-- ====================== ../Barcode ============================== -->\n\n",
